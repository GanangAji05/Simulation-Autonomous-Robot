<?xml version="1.0"?>

   <robot name="sapi" xmlns:xacro="http://www.ros.org/wiki/xacro">

    <!-- <xacro:include filename="$(find robot_des)/models/models.gazebo"/> -->
    <xacro:include filename="$(find description_robot)/models/material.xacro"/>
    <xacro:include filename="$(find description_robot)/models/param.xacro"/>
    <xacro:include filename="$(find description_robot)/models/model.gazebo"/>

    <link name='chassis'>
        <pose>0 0 0.1 0 0 0</pose>
            <inertial>
                <origin xyz="${bodybawah}" rpy="0.0 0.0 0.0"/>
                <mass value="5.0"/>
                <inertia 
                    ixx="0.0" ixy="0.0" 
                    ixz="0.0" iyy="0.0" 
                    iyz="0.0" izz="0.0"/>
            </inertial>

            <!-- body lingkaran bawah -->
            <visual name='chassis_visual_1'>
                <origin xyz="${bodybawah}" rpy="0.0 0.0 0.0"/>
                <geometry>
                    <!-- <cylinder radius="${width}" length="${bodylen}"/> -->
                    <box size="0.4 0.23 0.02"/>
                </geometry>
                <material name="grey"/>    
            </visual>

            <collision name='chasis_collosin_1'>
                <origin xyz="${bodybawah}" rpy="0.0 0.0 0.0"/>
                <geometry>
                    <!-- <cylinder radius="${width}" length="${bodylen}"/> -->
                    <box size="0.4 0.23 0.02"/>
                </geometry>
            </collision>

            <!-- body linkaran tengah -->
            <visual name='chassis_visual_2'>
                <origin xyz="${bodytengah}" rpy="0.0 0.0 0.0"/>
                    <geometry>
                        <cylinder radius="${width1}" length="${bodylen1}"/>
                    </geometry>
                <material name="grey"/>    
            </visual>

            <collision name='chasis_collosin_2'>
                <origin xyz="${bodytengah}" rpy="0.0 0.0 0.0"/>
                <geometry>
                    <cylinder radius="${width1}" length="${bodylen1}"/>
                </geometry>
            </collision>

            <!-- Jetson -->
            <visual name='chassis_visual_1'>
                <origin xyz="${Jetson}" rpy="0.0 0.0 0.0"/>
                <geometry>
                    
                    <box size="0.2 0.12 0.08"/>
                </geometry>
                <material name="white"/>    
            </visual>

            <collision name='chasis_collosin_1'>
                <origin xyz="${Jetson}" rpy="0.0 0.0 0.0"/>
                <geometry>
                   
                    <box size="0.2 0.12 0.08"/>
                </geometry>
            </collision>

            <!-- body atas -->
            <visual name='chassis_visual_3'>
                <origin xyz="${bodyatas}" rpy="0.0 0.0 0.0"/>
                    <geometry>
                        <cylinder radius="${width1}" length="${bodylen1}"/>
                    </geometry>
                <material name="grey"/>    
            </visual>

            <collision name='chasis_collosin_3'>
                <origin xyz="${bodyatas}" rpy="0.0 0.0 0.0"/>
                <geometry>
                    <cylinder radius="${width1}" length="${bodylen1}"/>
                </geometry>
            </collision>

            <!-- body -->
            <visual name='chassis_visual_depan'>
                <origin xyz="${bodybawah_depan}" rpy="0.0 0.0 0.0"/>
                <geometry>
                    <cylinder radius="${width}" length="${bodylen}"/>                 
                </geometry>
                <material name="grey"/>    
            </visual>

            <collision name='chasis_collosin_depan'>
                <origin xyz="${bodybawah_depan}" rpy="0.0 0.0 0.0"/>
                <geometry>
                    <cylinder radius="${width}" length="${bodylen}"/>
                </geometry>
            </collision>

            <visual name='chassis_visual_depan'>
                <origin xyz="0.16 0.0 0.0" rpy="0.0 0.0 0.0"/>
                <geometry>
                    <box size="0.1 0.3 0.02"/>                
                </geometry>
                <material name="grey"/>    
            </visual>

            <collision name='chasis_collosin_depan'>
                <origin xyz="0.16 0.0 0.0" rpy="0.0 0.0 0.0"/>
                <geometry>
                    <box size="0.1 0.3 0.02"/>
                </geometry>
            </collision>

            <visual name='chassis_visual_belakang'>
                <origin xyz="${bodybawah_belakang}" rpy="0.0 0.0 0.0"/>
                <geometry>
                    <cylinder radius="${width}" length="${bodylen}"/>                 
                </geometry>
                <material name="grey"/>    
            </visual>

            <collision name='chasis_collosin_belakang'>
                <origin xyz="${bodybawah_belakang}" rpy="0.0 0.0 0.0"/>
                <geometry>
                    <cylinder radius="${width}" length="${bodylen}"/>
                </geometry>
            </collision>

            <visual name='chassis_visual_belakang'>
                <origin xyz="-0.16 0.0 0.0" rpy="0.0 0.0 0.0"/>
                <geometry>
                    <box size="0.1 0.3 0.02"/>                
                </geometry>
                <material name="grey"/>    
            </visual>

            <collision name='chasis_collosin_belakang'>
                <origin xyz="-0.16 0.0 0.0" rpy="0.0 0.0 0.0"/>
                <geometry>
                    <box size="0.1 0.3 0.02"/>
                </geometry>
            </collision>
            <!-- END BODY -->

            <!-- Spacer body -->
            <visual name='spacer'>
                <origin xyz="0.1 0.1 0.15" rpy="0.0 0.0 0.0"/>
                    <geometry>
                        <cylinder radius="${width_spacer}" length="${bodylen_spacer}"/>
                    </geometry>
                    <material name="brown"/>    
            </visual>

            <collision name='spacer'>
                <origin xyz="0.1 0 0.15" rpy="0.0 0.0 0.0"/>
                    <geometry>
                        <cylinder radius="${width_spacer}" length="${bodylen_spacer}"/>
                    </geometry>
            </collision>

            <visual name='spacer_2'>
                <origin xyz="-0.1 -0.1 0.15" rpy="0.0 0.0 0.0"/>
                    <geometry>
                        <cylinder radius="${width_spacer}" length="${bodylen_spacer}"/>
                    </geometry>
                    <material name="brown"/>             
            </visual>

            <collision name='spacer_2'>
                <origin xyz="-0.1 -0.1 0.15" rpy="0.0 0.0 0.0"/>
                    <geometry>
                        <cylinder radius="${width_spacer}" length="${bodylen_spacer}"/>
                    </geometry> 
            </collision>

            <visual name='spacer_3'>
                <origin xyz="0.1 -0.1 0.15" rpy="0.0 0.0 0.0"/>
                    <geometry>
                        <cylinder radius="${width_spacer}" length="${bodylen_spacer}"/>
                    </geometry>
                    <material name="brown"/>             
            </visual>

            <collision name='spacer_3'>
                <origin xyz="0.1 -0.1 0.15" rpy="0.0 0.0 0.0"/>
                    <geometry>
                        <cylinder radius="${width_spacer}" length="${bodylen_spacer}"/>
                    </geometry> 
            </collision>

            <visual name='spacer_4'>
                <origin xyz="-0.1 0.1 0.15" rpy="0.0 0.0 0.0"/>
                    <geometry>
                        <cylinder radius="${width_spacer}" length="${bodylen_spacer}"/>
                    </geometry>
                    <material name="brown"/>            
            </visual>

            <collision name='spacer_4'>
                <origin xyz="-0.1 0.1 0.15" rpy="0.0 0.0 0.0"/>
                    <geometry>
                        <cylinder radius="${width_spacer}" length="${bodylen_spacer}"/>
                    </geometry> 
            </collision>
            <!-- END SPACER -->

            <!-- Box camera -->
            <visual name="box_camera">
                <origin xyz="0.265 0 0.07" rpy="0.0 0.0 0.0"/>
                <geometry>
                    <box size="0.02 0.15 0.05"/>
                </geometry>
                <material name="white"/>
            </visual>

            <collision>
                <origin xyz="0.265 0 0.07" rpy="0.0 0.0 0.0"/>
                <geometry>
                    <box size="0.02 0.15 0.05"/>
                </geometry>
            </collision>

            <visual name="cycle_camera_1">
                <origin xyz="0.265 0.08 0.07" rpy="1.5707 0 1.5707"/>
                <geometry>
                    <cylinder radius="0.025" length="0.02"/>
                </geometry>
                <material name="white"/>
            </visual>

            <collision>
                <origin xyz="0.265 0.08 0.07" rpy="1.5707 0 1.5707"/>
                <geometry>
                    <cylinder radius="0.025" length="0.02"/>
                </geometry>
            </collision>

            <visual name="cycle_camera_1">
                <origin xyz="0.265 -0.08 0.07" rpy="1.5707 0 1.5707"/>
                <geometry>
                    <cylinder radius="0.025" length="0.02"/>
                </geometry>
                <material name="white"/>
            </visual>

            <collision>
                <origin xyz="0.265 -0.08 0.07" rpy="1.5707 0 1.5707"/>
                <geometry>
                    <cylinder radius="0.025" length="0.02"/>
                </geometry>
            </collision>

            <visual name="tiang">
                <origin xyz="0.265 0 0.03" rpy="0.0 0.0 0.0"/>
                <geometry>
                    <box size="0.02 0.05 0.05"/>
                </geometry>
                <material name="white"/>
            </visual>

            <collision>
                <origin xyz="0.265 0 0.03" rpy="0.0 0.0 0.0"/>
                <geometry>
                    <box size="0.02 0.05 0.05"/>
                </geometry>
            </collision>
            <!-- END Box -->


             <!-- Roda penyeimbang depan -->
            <visual name='cirle_front'>
                <origin xyz="0.26 0 -0.05" rpy="0.0 0.0 0.0"/>
                <geometry>
                    <sphere radius="0.05"/>
                </geometry>
                <material name="black"/> 
            </visual>

            <collision name='collisin_circle_front'>
                <origin xyz="0.26 0 -0.05" rpy="0.0 0.0 0.0"/>
                <geometry>
                    <sphere radius="0.05"/>
                </geometry>
            </collision>

            <!-- Roda penyeimbang belakang -->
            <visual name='cirle_rear'>
                <origin xyz="-0.26 0 -0.050" rpy="0.0 0.0 0.0"/>
                <geometry>
                    <sphere radius="0.05"/>
                </geometry>
                <material name="black"/> 
            </visual>

            <collision name='collisin_circle_rear'>
                <origin xyz="-0.26 0 -0.050" rpy="0.0 0.0 0.0"/>
                <geometry>
                    <sphere radius="0.05"/>
                </geometry>
            </collision>
      </link>
    
      <!-- Roda kiri -->
      <link name="left_wheel">
        <inertial>
            <origin xyz="0 0 0" rpy="0 1.5707 1.5707"/>
            <mass value="5"/>
            <inertia
              ixx=".1" ixy="0.0" ixz="0.0"
              iyy=".1" iyz="0.0"
              izz=".1"/>
          </inertial>

        <visual name="left_wheel_visual">
            <origin xyz="0 0.0 0.0" rpy="0 1.5707 1.5707"/>
            <geometry>
              <cylinder radius="${wheels_radius}" length="${wheels_length}"/>
            </geometry>
            <material name="black"/> 
        </visual>
  
        <collision name="collision">
          <origin xyz="0 0.0 0.0" rpy="0 1.5707 1.5707"/>
          <geometry>
            <cylinder radius="${wheels_radius}" length="${wheels_length}"/>
          </geometry>
        </collision>
      </link>
    
      <!-- Roda kanan -->
      <link name="right_wheel">
        <inertial>
            <origin xyz="0 0 0" rpy="0 1.5707 1.5707"/>
            <mass value="5"/>
            <inertia
              ixx=".1" ixy="0.0" ixz="0.0"
              iyy=".1" iyz="0.0"
              izz=".1"/>
        </inertial>

        <visual name="right_wheel_visual">
            <origin xyz="0 0.0 0.0" rpy="0 1.5707 1.5707"/>
            <geometry>
                <cylinder radius="${wheels_radius}" length="${wheels_length}"/>
            </geometry>
            <material name="black"/> 
        </visual>

        <collision name="collision">
          <origin xyz="0 0.0 0.0" rpy="0 1.5707 1.5707"/>
          <geometry>
            <cylinder radius="${wheels_radius}" length="${wheels_length}"/>
          </geometry>
        </collision>
      </link>
    
    
      <joint type="continuous" name="left_wheel_hinge">
        <origin xyz="0.0 0.15 0.0" rpy="0 0 0"/>
        <!--origin xyz="0.1 0.13 0" rpy="0 1.5707 1.5707"/-->
        <child link="left_wheel"/>
        <parent link="chassis"/>
        <axis xyz="0 1 0" rpy="0 0 0"/>
        <!-- <limit effort="100" velocity="100"/> -->
        <joint_properties damping="0.0" friction="0.0"/>
      </joint>
    
      <joint type="continuous" name="right_wheel_hinge">
        <origin xyz="0 -0.15 0.0" rpy="0 0 0"/>
        <!--origin xyz="0.1 -0.13 0" rpy="0 1.5707 1.5707"/-->
        <child link="right_wheel"/>
        <parent link="chassis"/>
        <axis xyz="0 1 0" rpy="0 0 0"/>
        <!-- <limit effort="100" velocity="100"/> -->
        <joint_properties damping="0.0" friction="0.0"/>
      </joint>

      <link name="camera">
        <collision>
          <origin xyz="0 0 0" rpy="1.5707 0 1.5707"/>
          <geometry>
          
            <cylinder radius="0.013" length="0.01"/>
          </geometry>
        </collision>
    
        <visual>
          <origin xyz="0 0 0" rpy="1.5707 0 1.5707"/>
          <geometry>
          
            <cylinder radius="0.013" length="0.01"/>
          </geometry>
          <material name="black"/>
        </visual>
    
        <inertial>
          <mass value="${cameraMass}" />
          <origin xyz="0 0 0" rpy="0 0 0"/>
          
          <inertia ixx="1e-6" ixy="0" ixz="0" iyy="1e-6" iyz="0" izz="1e-6" />
        </inertial>
      </link>
    
      <joint name="camera_joint" type="fixed">
        <axis xyz="0 1 0" />
        <origin xyz="0.275 0 0.07" rpy="0 0 0"/>
        <parent link="chassis"/>
        <child link="camera"/>
      </joint>

      <!-- Add a Hokuyo Laser -->
    <link name="laser_link">
      <collision>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <box size="${x_hokuyo} ${y_hokuyo} ${z_hokuyo}"/>
        </geometry>
      </collision>

      <visual>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="package://description_robot/meshes/hokuyo.dae"/>
          <!-- <box size="${x_hokuyo} ${y_hokuyo} ${z_hokuyo}"/> -->
        </geometry>
      </visual>

      <inertial>
        <mass value="1e-5" />
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <inertia ixx="1e-6" ixy="0" ixz="0" iyy="1e-6" iyz="0" izz="1e-6" />
      </inertial>
    </link>
    
    <joint name="hokuyo_joint" type="fixed">
      <axis xyz="0 1 0" />
      <origin xyz="0.0 0 0.33" rpy="0 0 0"/>
      <parent link="chassis"/>
      <child link="laser_link"/>
    </joint>


  </robot>